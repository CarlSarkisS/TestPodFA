  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  
  pod 'Firebase/Analytics', '~> 7.1.0'
  pod 'Firebase/Crashlytics', '~> 7.1.0'

end

target 'TestPodFAApp' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  pod 'PodFA', :path => './'

end
